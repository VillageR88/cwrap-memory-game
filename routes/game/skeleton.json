{
  "head": {
    "link": [
      {
        "rel": "shortcut icon",
        "href": "../favicon.ico",
        "type": "image/x-icon"
      }
    ]
  },
  "classroom": [
    {
      "name": "legend",
      "type": "element",
      "style": "color: #7191A5; font-size: 20px; font-weight: bold; padding-inline: unset;"
    },
    {
      "name": "body > main > div:nth-of-type(2) > button",
      "type": "custom",
      "style": "background-color: #304859; border-radius: 50%; box-sizing: border-box; color: #FCFCFC; flex-grow: 1; font-size: 56px; font-weight: bold; padding: 0; transition: 300ms;"
    },
    {
      "name": "body > main > div:nth-of-type(2) > button:hover",
      "type": "custom",
      "style": "background-color: #6395B8;"
    },
    {
      "name": "body > main > div:nth-of-type(2) > button.clicked",
      "type": "custom",
      "style": "background-color: #FDA214; transition: 150ms;"
    },
    {
