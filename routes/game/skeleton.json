  "element": "body",
  "style": "align-items: center; background-color: #FCFCFC; box-sizing: border-box; display: flex; flex-direction: column; font-family: Atkinson Hyperlegible, serif; justify-content: center; margin: 0; min-height: 100dvh; padding: 40px;",
  "children": [
    {
      "element": "nav",
      "style": "display: flex; width: 100%;",
      "children": [
        {
          "element": "div",
          "style": "align-items: center; display: flex; justify-content: space-between; margin-inline: auto; max-width: 1110px; width: 100%;",
          "children": [
            {
              "element": "h1",
              "text": "memory",
              "style": "color: #152938;"
            },
            {
              "element": "div",
              "style": "display: flex; gap: 16px; max-width: 292px; width: 100%;",
              "children": [
                {
                  "element": "button",
                  "attributes": {
                    "id": "restart"
                  },
                  "text": "Restart",
                  "extend": [
                    {
                      "extension": ":hover",
                      "style": "background-color: #FFB84A;"
                    }
                  ],
                  "style": "background-color: #FDA214; border-radius: 26px; color: #FCFCFC; cursor: pointer; font-family: Atkinson Hyperlegible, serif; font-size: 20px; font-weight: bold; max-width: 127px; min-height: 52px; transition: 300ms; width: 100%;"
                },
                {
                  "element": "button",
                  "attributes": {
                    "id": "new-game"
                  },
                  "text": "New Game",
                  "extend": [
                    {
                      "extension": ":hover",
                      "style": "background-color: #6395B8; color: #FCFCFC;"
                    }
                  ],
                  "style": "background-color: #DFE7EC; border-radius: 26px; color: #304859; cursor: pointer; font-family: Atkinson Hyperlegible, serif; font-size: 20px; font-weight: bold; max-width: 149px; min-height: 52px; transition: 300ms; width: 100%;"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "element": "main",
      "style": "color: white; display: flex; flex-direction: column; max-width: 1110px; width: 100%;",
      "children": [
        {
          "element": "div",
          "extend": [
            {
              "extension": ".larger",
              "style": "gap: 16px; grid-template-columns: repeat(6, 1fr); height: 572px; margin: 85px auto 83px auto; max-width: 572px;"
            }
          ],
          "style": "display: grid; gap: 20px; grid-template-columns: repeat(4, 1fr); height: 523px; margin: 105px auto 122px auto; max-width: 532px; width: 100%;"
        }
      ]
    },
    {
      "element": "footer",
      "extend": [
        {
          "extension": "> div",
          "style": "align-items: center; background-color: #DFE7EC; border-radius: 10px; box-sizing: border-box; display: flex; height: 72px; justify-content: space-between; max-width: 255px; padding: 17px 24px 15px 21px; width: 100%;"
        },
        {
          "extension": "> div > h2:nth-of-type(1)",
          "style": "color: #7191A5; font-size: 18px; font-weight: bold; margin: 0;"
        },
        {
          "extension": "> div > p:nth-of-type(1)",
          "style": "color: #304859; font-size: 32px; font-weight: bold; margin: 0;"
        },
        {
          "extension": "> div > svg",
          "style": "fill: red;"
        },
        {
          "extension": "> div.turn",
          "style": "background-color: #FDA214;"
        },
        {
          "extension": "> div.turn > *:nth-of-type(1)",
          "style": "color: white;"
        }
      ],
      "style": "display: flex; gap: 30px; justify-content: center; margin-inline: auto; max-width: 1110px; min-height: 130px; width: 100%; width: 100%;"
    },
    {
      "element": "script",
      "attributes": {
        "type": "module",
        "src": "../static/scripts/game.js"
      }
    }
  ]
}