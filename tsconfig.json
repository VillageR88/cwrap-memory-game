        "target": "ES6",
        "module": "ES6",
        "outDir": "./static/scripts", // The folder where compiled JS will go
        "rootDir": "./static/typescript", // Your source TypeScript folder
        "strict": true
    },
    "include": [
        "static/typescript/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}